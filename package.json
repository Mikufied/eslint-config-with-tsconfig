{
    "name": "eslint-config-with-tsconfig",
    "version": "1.5.4",
    "description": "Mikufied's ESLint config",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.js"
        },
        "./helper": {
            "types": "./lib/helper.d.ts",
            "require": "./lib/helper.js"
        },
        "./tsconfig.base.json": "./tsconfig.base.json"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "tsconfig.base.json"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf lib",
        "dev": "tsup --watch",
        "format": "rome format . --write",
        "lint": "eslint --color . --ext .ts --fix",
        "prepublish": "publint && pnpm build"
    },
    "dependencies": {
        "@typescript-eslint/type-utils": "^5.59.8",
        "confusing-browser-globals": "^1.0.11",
        "consola": "^3.1.0",
        "eslint-config-galex": "^4.5.2",
        "eslint-define-config": "^1.20.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-case-police": "^0.6.1",
        "eslint-plugin-functional": "^5.0.8",
        "eslint-plugin-github": "^4.7.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react-refresh": "0.4.1",
        "eslint-plugin-regexp": "^1.15.0",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort": "^2.10.0",
        "get-tsconfig": "^4.6.0",
        "local-pkg": "^0.4.3"
    },
    "devDependencies": {
        "@types/confusing-browser-globals": "1.0.0",
        "@types/eslint": "8.40.0",
        "@types/node": "20.2.5",
        "eslint": "8.41.0",
        "publint": "0.1.12",
        "rome": "12.1.3",
        "tsup": "6.7.0",
        "type-fest": "3.11.1",
        "typescript": "5.0.4"
    },
    "peerDependencies": {
        "eslint": ">=8.40.0",
        "typescript": ">=5.0.4"
    },
    "engines": {
        "node": ">=18.16.0"
    }
}
