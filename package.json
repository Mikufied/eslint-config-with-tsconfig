{
    "name": "eslint-config-with-tsconfig",
    "version": "1.9.1",
    "description": "Rel1cx's ESLint config",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "require": "./lib/index.js"
        },
        "./helper": {
            "types": "./lib/helper.d.ts",
            "require": "./lib/helper.js"
        },
        "./tsconfig.base.json": "./tsconfig.base.json"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "tsconfig.base.json"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf lib",
        "dev": "tsup --watch",
        "format": "rome format . --write",
        "lint": "eslint --color . --ext .ts --fix",
        "prepublish": "publint && pnpm build"
    },
    "dependencies": {
        "consola": "^3.2.3",
        "eslint-config-relicx": "^0.3.1",
        "eslint-config-typed-fp": "^5.1.0",
        "eslint-plugin-case-police": "^0.6.1",
        "eslint-plugin-expect-type": "^0.2.3",
        "eslint-plugin-functional": "^5.0.8",
        "eslint-plugin-import": "npm:eslint-plugin-i@latest",
        "eslint-plugin-regexp": "^1.15.0",
        "eslint-plugin-rxjs": "^5.0.3",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-sort": "^2.10.0",
        "eslint-plugin-total-functions": "^7.0.7",
        "get-tsconfig": "^4.6.2"
    },
    "devDependencies": {
        "@types/eslint": "8.40.2",
        "@types/node": "20.4.0",
        "eslint": "8.44.0",
        "publint": "0.1.16",
        "rome": "12.1.3",
        "tsup": "7.1.0",
        "type-fest": "3.12.0",
        "typescript": "5.1.6"
    },
    "peerDependencies": {
        "eslint": ">=8.43.0",
        "typescript": ">=5.1.6"
    },
    "engines": {
        "node": ">=18.16.0"
    }
}
